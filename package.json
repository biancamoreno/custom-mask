{
  "name": "@biancamoreno/custom-mask",
  "version": "1.0.0",
  "description": "Flexible masking engine for inputs and data formatting in JavaScript/TypeScript.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biancamoreno/custom-mask.git"
  },
  "keywords": [
    "mask",
    "input",
    "formatting",
    "typescript",
    "react",
    "react-native"
  ],
  "author": "Bianca Moreno",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
